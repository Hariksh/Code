'''
class Node:
    def __init__(self, val=1):
        self.val = val
        self.left = None
        self.right = None
'''

def pos_node_sum(root):
    if root is None:
        return 0
    total = 0
    if root.val % 2 == 0:
        total = root.val
    else:
        total = 0
    return total + pos_node_sum(root.left) + pos_node_sum(root.right)
