def dfsOfGraph(n, edges):
    graph = [[] for _ in range(n)]

    for u,v in edges:
        graph[u].append(v)
        graph[v].append(u)

    visited = [False] * n
    res = []
    def dfs(node):
        visited[node] = True
        res.append(node)
        for nei in graph[node]:
            if not visited[nei]:
                dfs(nei)

    for i in range(n):
        if not visited[i]:
            dfs(i)
    
    return res
