def add(a, b):
    return a+b

def subtract(a, b):
    return a-b

def multiply(a, b):
    return a*b

def divide(a, b):
    return a/b

def reminder(a, b):
    return a%b

a = int(input())
b = int(input())

print("Addition:",add(a,b))
print("Subtraction:",subtract(a,b))
print("Multiplication:",multiply(a,b))
print("Division:", divide(a,b))
print("Remainder:" ,reminder(a,b))
