n = int(input())  # Number of elements in set1
set1 = set(int(input()) for _ in range(n))

m = int(input())  # Number of elements in set2
set2 = set(int(input()) for _ in range(m))


union = sorted(set1 | set2)  # Union using set1 | set2 or set1.union(set2)
print(union)

intersection = sorted(set1 & set2)  # Intersection using set1 & set2 or set1.intersection(set2)
print(intersection)
