n = int(input())

num_dict = {}

for _ in range(n):
    key = int(input())
    value = int(input())
    num_dict[key] = value

product_keys = 1
for key in num_dict.keys():
    product_keys *= key

product_values = 1
for value in num_dict.values():
    product_values *= value

print(product_keys)
print(product_values)
