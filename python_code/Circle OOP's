class Circle:
    def __init__(self, radius):
        self.radius = radius

    def area(self):
        pi = 3.14
        return pi * self.radius**2

    def circumference(self):
        pi = 3.14
        return 2 * pi * self.radius

if __name__ == "__main__":
    radius = float(input())
    circle = Circle(radius)

    area = circle.area()
    circumference = circle.circumference()

    print(area)
    print(circumference)
