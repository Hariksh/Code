def numbers():
    n = int(input())  
    positive_count = negative_count = even_count = odd_count = 0

    for i in range(n):
        number = int(input())
        if number > 0:
            positive_count += 1
        elif number < 0:
            negative_count += 1
        if number % 2 == 0:
            even_count += 1
        else:
            odd_count += 1

    print(positive_count, negative_count, even_count, odd_count)

numbers()
